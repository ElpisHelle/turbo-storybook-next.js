{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "encharted#build": {
      "dependsOn": ["^build", "$DOCS_URL"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "storybook": {
      "cache": false
    },
    "build:storybook": {
      "outputs": ["storybook-static/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
